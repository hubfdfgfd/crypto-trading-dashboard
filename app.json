    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "platforms": ["android"],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a1a2e"
      },
      "package": "com.buzer88.cryptodashboard",
      "versionCode": 1,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "allowBackup": false
    },
    "extra": {
      "eas": {
        "projectId": "131148a2-1b46-4a72-9c01-9b3f192ad24b"
      }
    }
  }
