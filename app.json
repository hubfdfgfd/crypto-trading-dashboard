{
  "expo": {
    "name": "Crypto Trading Dashboard",
    "slug": "crypto-trading-dashboard",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "scheme": "cryptodashboard",
    "icon": "./assets/icon.png",
    "updates": {
