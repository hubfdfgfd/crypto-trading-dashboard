{
  "expo": {
    "name": "Crypto Trading Dashboard",
    "slug": "crypto-trading-dashboard",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "updates": {
      "enabled": false
    },
    "splash": {
      "backgroundColor": "#1a1a2e",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "platforms": ["ios", "android"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.buzer88.cryptodashboard"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1a1a2e"
      },
      "package": "com.buzer88.cryptodashboard",
      "versionCode": 1,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-XXXX~YYYY"
      },
      "allowBackup": false,
      "usesCleartextTraffic": true
    },
    "extra": {
      "eas": {
        "projectId": "131148a2-1b46-4a72-9c01-9b3f192ad24b"
      }
    },
    "cli": {
      "appVersionSource": "remote"
    }
  }
}
